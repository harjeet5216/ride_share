<p id="notice"><%= notice %></p>

<h1>Users</h1>

<br>


<table class="table table-hover">
  <thead>
    <tr>
      <th>Email ID</th>
      <th>User ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.id %></td>
        <td><%= link_to 'Show', controller: "users", action: "show", id: user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

